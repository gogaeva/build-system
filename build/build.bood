go_binary {
  // Module (and output) name.
  name: "bood",

  // Go package to build.
  pkg: "github.com/gogaeva/build-system/build/cmd/bood",

  // Build inputs.
  srcs: ["**/*.go", "../go.mod"],
  srcsExclude: ["**/*_test.go"],
  
  // Package to test.
  testPkg: "github.com/gogaeva/build-system/build/gomodule",

  //Test inputs.
  testSrcs: ["**/*_test.go"],
}
